{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "requirements": "requirements.txt",
        "installCommand": "sudo apt-get update && sudo apt-get install -y libsqlite3-dev"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
